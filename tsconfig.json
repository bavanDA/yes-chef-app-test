{
	"compilerOptions": {
		"target": "es2016",
		"module": "ESNext",
		"rootDir": "./",
		"moduleResolution": "Node",
		"allowJs": true,
		"sourceMap": true,
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": ["**/*.ts"],    
	"exclude": ["node_modules",	"dist",]	
}
